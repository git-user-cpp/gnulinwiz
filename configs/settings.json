{
  "terminal": {
    "dock": "right"
  },
  "base_keymap": "VSCode",
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "Ayu Dark"
  },
  "autosave": {
    "after_delay": {
      "milliseconds": 2000
    }
  },
  "inlay_hints": {
    "enabled": true,
    "show_type_hints": true,
    "show_parameter_hints": true,
    "show_other_hints": true,
    "edit_debounce_ms": 700,
    "scroll_debounce_ms": 50
  },
  "cursor_blink": false,
  "vim_mode": true,
  "ui_font_size": 12,
  "buffer_font_size": 12,
  "confirm_quit": true,
  "scroll_sensitivity": 1.5,
  "auto_fold_dirs": false,
  "cursor_shape": "underline"
}
